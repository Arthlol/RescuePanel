@model MvcApplication1.Models.Employee

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Employee</legend>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.UserId)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.UserId)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.JobTitleId)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.JobTitleId)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.PersonalNumber)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.PersonalNumber)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.WorkPhone)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.WorkPhone)
    </div>
</fieldset>
@if (Model.Driver!=null)
{
    @Html.ActionLink("Информация водителя", "Edit", "Driver", new { id = WebSecurity.CurrentUserId }, null);
}

@if (Model.Operator != null)
{
    @Html.ActionLink("Информация оператора", "Edit", "Driver", new { id = WebSecurity.CurrentUserId }, null);
}
@if (Model.Rescuer != null)
{
    @Html.ActionLink("Информация спасателя", "Edit", "Driver", new { id = WebSecurity.CurrentUserId }, null);
}








<div>
    <a href="javascript:history.back(1)">назад</a>
</div>


